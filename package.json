{
	"name": "kuui-react",
	"private": false,
	"version": "0.0.1",
	"type": "commonjs",
	"scripts": {
		"build": "webpack",
		"format": "npx prettier --write src/",
		"push": "npx prettier --write src/ && git add . && git commit -m '.' && git push origin webpack",
		"npmjs": "webpack && npm version patch --no-git-tag-version && npm publish"
	},
	"dependencies": {
		"clsx": "^1.2.1"
	},
	"peerDependencies": {
		"react": "*"
	},
	"devDependencies": {
		"@storybook/react": "^7.3.2",
		"@types/react": "^18.0.37",
		"css-loader": "^6.8.1",
		"ignore-loader": "^0.1.2",
		"mini-css-extract-plugin": "^2.7.6",
		"prettier": "^2.8.8",
		"react": "^18.2.0",
		"sass": "^1.63.6",
		"sass-loader": "^13.3.2",
		"terser-webpack-plugin": "^5.3.9",
		"ts-loader": "^9.4.4",
		"typescript": "^5.0.2",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.umd.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		},
		"./styles": {
			"import": "./dist/index.css",
			"require": "./dist/index.css"
		}
	}
}
