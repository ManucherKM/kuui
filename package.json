{
	"name": "kuui-react",
	"private": false,
	"version": "0.0.2",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"format": "npx prettier --write src/",
		"push": "npx prettier --write src/ && git add . && git commit -m '.' && git push origin kuui-react",
		"publish": "vite build && npm version patch --no-git-tag-version && npm publish"
	},
	"dependencies": {
		"clsx": "^1.2.1"
	},
	"peerDependencies": {
		"react": "*"
	},
	"devDependencies": {
		"@types/react": "^18.0.37",
		"@types/react-dom": "^18.0.11",
		"@vitejs/plugin-react": "^4.0.0",
		"prettier": "^2.8.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.63.6",
		"typescript": "^5.0.2",
		"vite": "^4.3.9",
		"vite-plugin-dts": "^3.2.0"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.umd.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		},
		"./styles": {
			"import": "./dist/style.css",
			"require": "./dist/style.css"
		}
	}
}
